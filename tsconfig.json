{
  "compilerOptions": {
    "outDir": "build",
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
